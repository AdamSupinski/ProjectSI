<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="org.aiclasses.knapsack.ui.controller.DataViewController">
    <children>
        <Label layoutX="406.0" layoutY="14.0" text="Dane przedmiotu"/>
        <Button fx:id="calculateButton" defaultButton="true" layoutX="341.0" layoutY="318.0" mnemonicParsing="false"
                onAction="#handleCalculate" prefHeight="34.0" prefWidth="115.0" text="Oblicz"
                AnchorPane.bottomAnchor="25.0" AnchorPane.rightAnchor="75.0"/>
        <GridPane alignment="CENTER" layoutX="348.0" layoutY="30.0" prefHeight="302.0" prefWidth="249.0"
                  AnchorPane.bottomAnchor="68.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="30.0">
            <children>
                <TextField fx:id="weightTextField" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                           GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS"/>
                <TextField fx:id="valueTextField" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                           GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER"
                           GridPane.vgrow="ALWAYS"/>
                <Button fx:id="addItemButton" mnemonicParsing="false" onAction="#handleAddItem" prefHeight="25.0"
                        prefWidth="90.0" text="Dodaj" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                        GridPane.rowIndex="2" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS"/>
                <Button fx:id="deleteItemButton" mnemonicParsing="false" onAction="#handleDeleteItem" prefHeight="25.0"
                        prefWidth="90.0" text="Usuń" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Waga"/>
                <Label text="Wartość" GridPane.rowIndex="1"/>
                <Label text="Liczba iteracji" GridPane.rowIndex="3"/>
                <Label text="Współczynnik krzyżowania" textAlignment="CENTER" wrapText="true" GridPane.rowIndex="5"/>
                <Label text="Prawdopodob. mutacji" textAlignment="CENTER" wrapText="true" GridPane.rowIndex="6"/>
                <TextField fx:id="iterationsTextField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <TextField fx:id="crossoverTextField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <TextField fx:id="mutationTextField" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
                <Label text="Rozmiar populacji" textAlignment="CENTER" wrapText="true" GridPane.rowIndex="4"/>
                <TextField fx:id="populationTextField" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <TextField fx:id="knapsackTextField" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
                <Label text="Pojemność plecaka" textAlignment="CENTER" wrapText="true" GridPane.rowIndex="7"/>
            </children>
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="98.0" minWidth="10.0" prefWidth="86.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="120.0" minWidth="10.0"
                                   prefWidth="113.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS"/>
            </rowConstraints>
        </GridPane>
        <TableView fx:id="itemTableView" layoutX="10.0" layoutY="12.0" prefHeight="377.0" prefWidth="332.0"
                   AnchorPane.bottomAnchor="11.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="258.0"
                   AnchorPane.topAnchor="12.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="weightTableColumn" editable="false" prefWidth="152.0" text="Waga"/>
                <TableColumn fx:id="valueTableColumn" editable="false" prefWidth="150.0" text="Wartość"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
